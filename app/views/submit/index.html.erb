    <section class="submit-form-container">
      <h1>Submit Your Event</h1>

      <form class="submission-form" accept-charset="UTF-8" action="https://formkeep.com/f/ebbedece2e8f" method="POST">
        <input type="hidden" name="utf8" value="âœ“">

        <label for="event">Event Name</label>
        <input type="text" name="name" placeholder="Name of your event" required="">

        <label for="description">Description of Event</label>
        <textarea type="text" name="description" placeholder="What is this event about? Cost, is food provided, etc." required=""></textarea>


        <label for="how_often">Date + Time</label>
        <textarea type="text" name="occurrence" placeholder="e.g., 'Every Third Thursday of the Month'" required=""></textarea>

        <label for="recurring">Is this Event recurring?</label>
        <input type="radio" name="recurring" value="Yes" checked=""> <span>Yes</span>
        <input type="radio" name="recurring" value="No"> <span>No</span>

        <label for="company_website">Event URL</label>
        <input  type="url" name="url" id="event_url" placeholder="http://" validate="" required="">

        <label for="location">Location Name</label>
        <input type="text" name="location_name" placeholder="e.g., 'Hogwarts'" required="">

        <label for="street_address">Street Address</label>
        <input type="text" name="street_address" placeholder="e.g., '123 Dumbledore St'" required="">

        <label for="city">City</label>
        <input type="text" name="city" placeholder="e.g., 'Dayton'" required="">

        <label for="States">State</label>
